package entities;

public class Ativo {

	private String name;
	private String cod;
	private Double price;
	
	public Ativo(String name, String codigo, Double valor) {
		super();
		this.name = name;
		this.codigo = codigo;
		this.valor = valor;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCodigo() {
		return codigo;
	}

	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}

	public Double getValor() {
		return valor;
	}

	public void setValor(Double valor) {
		this.valor = valor;
	}
	
	
	
	
}
